{
  "name": "strategy-builder",
  "version": "1.0.0",
  "description": "MCP server for trading strategies using Claude Desktop as client",
  "main": "dist/index.js",
  "keywords": [
    "mcp",
    "model-context-protocol",
    "trading",
    "claude",
    "solana",
    "aws"
  ],
  "author": "",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": ""
  },
  "scripts": {
    "build": "tsc",
    "start": "node dist/src/server/server.js",
    "start:dev": "cross-env NODE_ENV=production ts-node src/server/server.ts",
    "start:mcp": "node dist/src/index.js",
    "dev": "cross-env NODE_ENV=development ts-node src/server/server.ts",
    "dev:mcp": "ts-node src/index.ts",
    "postinstall": "prisma generate",
    "deploy": "npm run build && npx prisma migrate deploy",
    "test": "jest",
    "test:unit": "jest --testPathPattern=\\.test\\.(ts|js)$",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:connection": "node test-connection.js",
    "test:connection:ts": "ts-node test-connection.ts",
    "test:simple": "ts-node test-simple-connection.ts",
    "test:trading": "ts-node test-trading-functions.ts",
    "test:confirmation": "ts-node test-transaction-confirmation.ts",
    "test:counter": "node test-aws-counter.js",
    "deploy:dev": "node scripts/deploy.js development",
    "deploy:prod": "node scripts/deploy.js production",
    "deploy:staging": "node scripts/deploy.js staging",
    "wallet:generate": "node scripts/generate-test-wallet.js",
    "env:init": "node scripts/quick-setup.js",
    "env:setup": "node scripts/setup-mainnet.js",
    "env:validate": "node scripts/validate-env.js",
    "env:test": "node scripts/test-mainnet.js",
    "security:setup": "node scripts/setup-security.js",
    "test:security": "node test-security-features.js",
    "test:quick": "node quick-test.js",
    "test:deploy": "node test-aws-deployment.js",
    "test:gemini": "node test-gemini-api.js",
    "clean": "rimraf dist",
    "prebuild": "npm run clean"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.10.0",
    "@aws-sdk/client-cloudwatch-logs": "^3.478.0",
    "@aws-sdk/client-dynamodb": "^3.478.0",
    "@aws-sdk/client-ecr": "^3.478.0",
    "@aws-sdk/client-ecs": "^3.478.0",
    "@aws-sdk/client-elastic-load-balancing-v2": "^3.478.0",
    "@aws-sdk/client-eventbridge": "^3.478.0",
    "@aws-sdk/client-iam": "^3.478.0",
    "@aws-sdk/client-lambda": "^3.478.0",
    "@aws-sdk/client-secrets-manager": "^3.478.0",
    "@aws-sdk/lib-dynamodb": "^3.478.0",
    "@coral-xyz/anchor": "^0.32.1",
    "@google/generative-ai": "^0.24.1",
    "@modelcontextprotocol/sdk": "^1.20.1",
    "@prisma/client": "^6.17.1",
    "@solana/spl-token": "^0.4.14",
    "@solana/web3.js": "^1.98.4",
    "@types/bn.js": "^5.2.0",
    "@types/express": "^4.17.23",
    "@types/node": "^20.19.17",
    "@types/uuid": "^10.0.0",
    "bcrypt": "^6.0.0",
    "bip39": "^3.1.0",
    "bs58": "^5.0.0",
    "cors": "^2.8.5",
    "dotenv": "^16.6.1",
    "ed25519-hd-key": "^1.3.0",
    "express": "^4.18.2",
    "express-rate-limit": "^8.1.0",
    "fireblocks-sdk": "^5.38.0",
    "helmet": "^8.1.0",
    "jsonwebtoken": "^9.0.2",
    "jszip": "^3.10.1",
    "rimraf": "^6.0.1",
    "socket.io": "^4.8.1",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "technicalindicators": "^3.1.0",
    "uuid": "^13.0.0",
    "ws": "^8.18.2",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/bcrypt": "^6.0.0",
    "@types/bs58": "^4.0.4",
    "@types/dotenv": "^6.1.1",
    "@types/jest": "^29.5.14",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/socket.io": "^3.0.1",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.8",
    "@types/ws": "^8.18.1",
    "cross-env": "^10.1.0",
    "jest": "^29.7.0",
    "prisma": "^6.17.1",
    "socket.io-client": "^4.8.1",
    "ts-jest": "^29.4.4",
    "ts-node": "^10.9.1",
    "typescript": "^5.3.3"
  }
}
