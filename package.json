{
  "name": "strategy-builder",
  "version": "1.0.0",
  "description": "MCP server for trading strategies using Claude Desktop as client",
  "main": "dist/index.js",
  "keywords": [
    "mcp",
    "model-context-protocol",
    "trading",
    "claude",
    "solana",
    "aws"
  ],
  "author": "",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": ""
  },
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "ts-node src/index.ts",
    "test": "node test-server.js",
    "test:counter": "node test-aws-counter.js",
    "deploy:dev": "node scripts/deploy.js development",
    "deploy:prod": "node scripts/deploy.js production",
    "deploy:staging": "node scripts/deploy.js staging",
    "env:init": "cp .env.example .env && echo 'Please configure .env file with your settings'",
    "env:validate": "node -e \"require('./scripts/deploy.js').validateEnvironment(process.env.NODE_ENV || 'development')\"",
    "test:deploy": "node test-aws-deployment.js",
    "clean": "rimraf dist",
    "prebuild": "npm run clean"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.10.0",
    "@aws-sdk/client-cloudwatch-logs": "^3.478.0",
    "@aws-sdk/client-dynamodb": "^3.478.0",
    "@aws-sdk/client-ecr": "^3.478.0",
    "@aws-sdk/client-ecs": "^3.478.0",
    "@aws-sdk/client-elastic-load-balancing-v2": "^3.478.0",
    "@aws-sdk/client-eventbridge": "^3.478.0",
    "@aws-sdk/client-iam": "^3.478.0",
    "@aws-sdk/client-lambda": "^3.478.0",
    "@aws-sdk/lib-dynamodb": "^3.478.0",
    "@solana/web3.js": "^1.87.6",
    "bs58": "^5.0.0",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "jszip": "^3.10.1",
    "rimraf": "^6.0.1",
    "socket.io": "^4.8.1",
    "ws": "^8.18.2",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/bs58": "^4.0.4",
    "@types/dotenv": "^6.1.1",
    "@types/express": "^4.17.23",
    "@types/node": "^20.19.17",
    "@types/socket.io": "^3.0.1",
    "@types/ws": "^8.18.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.3.3"
  }
}
